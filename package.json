{
  "title": "gulp-scss-lint-stylish",
  "name": "gulp-scss-lint-stylish",
  "version": "1.0.1",
  "homepage": "https://github.com/roeldev/gulp-scss-lint-stylish",
  "description": "Stylish reporter for gulp-scss-lint, following the visual style of jshint-stylish",
  "author": "Roel Schut (http://roelschut.nl)",
  "license": "GPL-2.0+",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/roeldev/gulp-scss-lint-stylish.git"
  },
  "bugs": "https://github.com/roeldev/gulp-scss-lint-stylish/issues",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "setup": "npm install",
    "start": "npm install && gulp watch",
    "test": "mocha --reporter spec --check-leaks",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter spec --check-leaks --globals store@sparkles",
    "depcheck": "depcheck --ignores=mocha-lcov-reporter,jshint-stylish"
  },
  "dependencies": {
    "gulp-util": "^3.0.6",
    "lodash": "^4.6.1",
    "log-symbols": "^1.0.2",
    "plur": "^2.1.2",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "gulp": "^3.9.0",
    "gulp-jscs": "^3.0.1",
    "gulp-jscs-stylish": "^1.2.1",
    "gulp-jshint": "^2.0.0",
    "gulp-mocha": "^2.1.3",
    "gulp-scss-lint": "^0.3.9",
    "istanbul": "^0.4.2",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "log-interceptor": "^1.0.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "1.2.0",
    "run-sequence": "^1.1.4"
  },
  "keywords": [
    "gulpfriendly",
    "gulp-scss-lint",
    "scss-lint",
    "scsslint",
    "scss",
    "sass",
    "reporter",
    "formatter",
    "format",
    "lint",
    "validate",
    "stylish",
    "elegant",
    "pretty"
  ]
}
