    "text-table": "^0.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "gulp": "^3.9.0",
    "gulp-jscs": "^3.0.1",
    "gulp-jscs-stylish": "^1.2.1",
    "gulp-jshint": "^1.11.2",
    "gulp-mocha": "^2.1.3",
    "gulp-scss-lint": "^0.2.4",
    "istanbul": "^0.3.21",
    "jscs": "^2.2.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "log-interceptor": "^1.0.0",
    "mocha": "^2.3.3",
    "mocha-lcov-reporter": "1.0.0",
    "run-sequence": "^1.1.4"
