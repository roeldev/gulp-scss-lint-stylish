{
  "title": "gulp-scss-lint-stylish",
  "name": "gulp-scss-lint-stylish",
  "version": "1.0.0",
  "homepage": "https://github.com/roeldev/gulp-scss-lint-stylish",
  "description": "Stylish reporter for gulp-scss-lint, following the visual style of jshint-stylish",
  "license": "GPL-2.0+",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/roeldev/gulp-scss-lint-stylish.git"
  },
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "setup": "npm install",
    "start": "npm install && gulp watch",
    "test": "mocha --reporter spec --check-leaks",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks"
  },
  "dependencies": {
    "gulp-util": "^3.0.6",
